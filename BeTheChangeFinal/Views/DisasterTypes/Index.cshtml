@model IEnumerable<BeTheChangeFinal.Models.DisasterType>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Disaster Categories</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>@item.DtypeName</td>
            <td>
                <a asp-action="Delete" asp-route-id="@item.DtypeName">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>



@model IEnumerable<BeTheChangeFinal.Models.Disaster>
<h3>Random Cause to Donate</h3>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DisasterName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DisasterDetails)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DisasterLocation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DisasterLink)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Urgency)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtypeNameNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DisasterName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DisasterDetails)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DisasterLocation)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DisasterLink)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Urgency)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DtypeNameNavigation.DtypeName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.DisasterId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.DisasterId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.DisasterId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>